{
  "private": true,
  "name": "dsr-blog",
  "version": "6.2.2",
  "description": "Personal blog powered by Hugo.",
  "license": "MPL-2.0",
  "author": "DSRKafuU <dsrkafuu@outlook.com> (https://dsrkafuu.su)",
  "scripts": {
    "hugo:dev": "hugo server",
    "hugo:gitpod": "hugo server --bind=\"0.0.0.0\"",
    "hugo:prod": "hugo --cleanDestinationDir",
    "rollup": "rollup -c",
    "minify": "node minify.js",
    "dev": "cross-env NODE_ENV=development env-cmd concurrently npm:hugo:dev",
    "gitpod": "cross-env NODE_ENV=development env-cmd concurrently npm:hugo:gitpod",
    "build": "cross-env NODE_ENV=production env-cmd --silent concurrently -m 1 npm:hugo:prod npm:minify npm:rollup"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "~7.14.7",
    "@sentry/browser": "~6.9.0",
    "@sentry/tracing": "~6.9.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "autoprefixer": "^10.3.1",
    "concurrently": "^6.2.0",
    "cross-env": "^7.0.3",
    "cssnano": "^5.0.6",
    "env-cmd": "^10.1.0",
    "eslint": "^7.29.0",
    "html-minifier-terser": "^5.1.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.5",
    "rollup": "^2.53.2",
    "rollup-plugin-terser": "^7.0.2"
  }
}
